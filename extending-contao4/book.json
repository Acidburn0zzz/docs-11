{
    "gitbook": "3.2.x",
    "plugins": [
        "bootstrap-callout",
        "edit-link",
        "anchorjs",
        "piwik",
        "toggle-chapters",
        "image-captions",
        "canonical-link"
    ],
    "links": {
        "sidebar": {
            "Contao Homepage": "https://contao.org",
            "Contao Documentation": "https://docs.contao.org"
        }
    },
    "pdf": {
        "toc": false
    },
    "pluginsConfig": {
        "callouts": {
            "showTypeInHeader": false
        },
        "edit-link": {
            "base": "https://github.com/contao/docs/edit/master/extending-contao4",
            "label": "Edit This Page"
        },
        "piwik": {
            "URL": "association.contao.org/piwik/",
            "siteId": 3
        },
        "canonical-link": {
            "baseURL": "https://docs.contao.org/books/extending-contao4/"
        },
        "anchorjs": {
            "selector": "h1,h2,h3,*:not(.callout) > h4,h5"
        }
    }
}
